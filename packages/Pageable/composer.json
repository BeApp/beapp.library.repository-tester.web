{
    "name": "beapp/repository-tester-pageable",
    "description": "Beapp pageable integration into testing library for Doctrine repositories",
    "type": "library",
    "license": "MIT",
    "keywords": ["symfony", "doctrine", "repository", "test"],
    "authors": [
        {
            "name": "Tanguy Reviller",
            "email": "t.reviller@beapp.fr"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "beapp/doctrine-pagination": "^1.0",
        "beapp/repository-tester-bundle": "^0.4"
    },
    "autoload": {
        "psr-4": {
            "Beapp\\RepositoryTesterBundle\\Pageable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Beapp\\RepositoryTesterBundle\\Pageable\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
